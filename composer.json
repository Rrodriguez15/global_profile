{
    "name": "rrodriguez15/global_profile",
    "description": "Reusable Drupal installation profile for Drupal 10 and 11, including common contrib and custom modules.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Robin Rodriguez",
            "email": "rodriguez.robin15@gmail.com"
        }
    ],
    "require": {
        "drupal/core": "^10 || ^11",
        "drupal/paragraphs": "^1.15",
        "drupal/layout_paragraphs": "^2.0",
        "drupal/webform": "^6.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/pathauto": "^1.11",
        "drupal/metatag": "^1.25",
        "drupal/redirect": "^1.9",
        "drupal/token": "^1.12",
        "drupal/simple_sitemap": "^5.0",
        "drupal/devel": "^5.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:rrodriguez15/global_profile.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rrodriguez15/my_custom_module.git"
        }
    ],
    "extra": {
        "installer-paths": {
            "web/profiles/custom/{$name}/": ["type:drupal-profile"],
            "web/modules/custom/{$name}/": ["type:drupal-custom-module"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}